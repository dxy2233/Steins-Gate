* {
	padding: 0rem;
	margin: 0rem;
	font-family: "微软雅黑";
}

body,
div,
ul {
	margin: 0rem;
	padding: 0rem;
}

a {
	text-decoration: none;
}

img {
	padding: 0rem;
	border: 0rem;
	margin: 0rem;
	display: block;
}
.box{
	width: 100%;
	height: auto;  
}
.combox{
	width: 100%;
	height: auto;
	position: relative; 
	overflow: hidden;
	margin-bottom:1.2rem ;
}
.mui-slider-item a img{
	height: 3.8rem;
}
/*地区筛选*/
.diqushaixuan{
	width: auto;
	height: 0.3rem; 
	position: absolute;
	top: 0.3rem;
	left: 0.3rem;
	font-size: 0.26rem;

	z-index: 9999;
}
#city_text2{  
	padding-right: 0.3rem;
	float: left;
	color: white !important;
	box-sizing: border-box;
} 
.diquilaiixbtn{
	float: left;
	width: 0.21rem;
	height: 0.13rem;
	position: relative;
	top: 0.15rem;
	left: -0.2rem;
}
.diquilaiixbtn img{
	width: 100%;
	height: 100%;
}

.footbox{
	width: 100%;
	height: 1.1rem;
	background: white;
	position: fixed;
	bottom: 0rem;
	left: 0rem;
	z-index: 999;
	padding-top: 0.1rem; 
	box-sizing: border-box;
}
.footbox a{
	float: left;
	width: 33%;
	height: 100%;
	padding-top: 0.5rem;
	color: #bdc2cb;
	font-size: 0.2rem;  
	text-align: center;
	box-sizing: border-box;
}
.footbox .a1{
	background: url(../img/shouye.png) no-repeat top;
	background-size: 0.4rem 0.4rem;
}
.footbox .a2{
	background: url(../img/dingdan.png) no-repeat top; 
	background-size: 0.4rem 0.4rem;
}
.footbox .a3{
	background: url(../img/wo.png) no-repeat top; 
	background-size: 0.4rem 0.4rem;
}

.footbox .a1_1{
	background: url(../img/shouye_lan.png) no-repeat top;
	background-size: 0.4rem 0.4rem;
}
.footbox .a2_2{
	background: url(../img/dingdan_lan.png) no-repeat top; 
	background-size: 0.4rem 0.4rem;
}
.footbox .a3_3{
	background: url(../img/wo_lan.png) no-repeat top; 
	background-size: 0.4rem 0.4rem;
}

 
.footbox:after{
	display: block;
	clear: both;
	content: "";
}
